<BorderPane prefHeight="850.0" prefWidth="800.0" styleClass="background" stylesheets="@../../shadowTheme.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.infinityminers.encrypter.guis.logic.KeyManagerMenuController">
   <center>
      <AnchorPane styleClass="background" BorderPane.alignment="CENTER">
